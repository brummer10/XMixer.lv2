
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.
@prefix time: <http://lv2plug.in/ns/ext/time#>.
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .
@prefix pprop: <http://lv2plug.in/ns/ext/port-props#> .
@prefix midi:  <http://lv2plug.in/ns/ext/midi#> .
@prefix patch: <http://lv2plug.in/ns/ext/patch#> .


<urn:name#me>
   a foaf:Person ;
   foaf:name "brummer" .

<urn:mixer>
   a lv2:Plugin ,
       lv2:MixerPlugin ;
   doap:maintainer <urn:name#me> ;
   doap:name "XMixer" ;
   lv2:project <urn:mixer> ;
   lv2:requiredFeature urid:map ;
   lv2:optionalFeature lv2:hardRTCapable ;
   lv2:minorVersion 1 ;
   lv2:microVersion 0 ;

guiext:ui <urn:mixer_ui> ;

rdfs:comment """
4 in 1 channel Mixer
""";

   lv2:port  [
       a lv2:AudioPort ,
          lv2:InputPort ;
      lv2:index 0 ;
      lv2:symbol "in1" ;
      lv2:name "In1" ;
   ], [
       a lv2:AudioPort ,
          lv2:InputPort ;
      lv2:index 1 ;
      lv2:symbol "in2" ;
      lv2:name "In2" ;
   ], [
       a lv2:AudioPort ,
          lv2:InputPort ;
      lv2:index 2 ;
      lv2:symbol "in3" ;
      lv2:name "In3" ;
   ], [
       a lv2:AudioPort ,
          lv2:InputPort ;
      lv2:index 3 ;
      lv2:symbol "in4" ;
      lv2:name "In4" ;
   ], [
      a lv2:AudioPort ,
           lv2:OutputPort ;
      lv2:index 4 ;
      lv2:symbol "out0" ;
      lv2:name "Out0" ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 5 ;
      lv2:symbol "Gain_1" ;
      lv2:name "Gain_1" ;
      lv2:default -2.0500000 ;
      lv2:minimum -60.000000 ;
      lv2:maximum 6.000000 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 6 ;
      lv2:symbol "Gain_2" ;
      lv2:name "Gain_2" ;
      lv2:default -2.0500000 ;
      lv2:minimum -60.000000 ;
      lv2:maximum 6.000000 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 7 ;
      lv2:symbol "Gain_3" ;
      lv2:name "Gain_3" ;
      lv2:default -2.0500000 ;
      lv2:minimum -60.000000 ;
      lv2:maximum 6.000000 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 8 ;
      lv2:symbol "Gain_4" ;
      lv2:name "Gain_4" ;
      lv2:default -2.0500000 ;
      lv2:minimum -60.000000 ;
      lv2:maximum 6.000000 ;
   ], [
      a lv2:OutputPort ,
          lv2:ControlPort ;
      lv2:index 9 ;
      lv2:symbol "VMeter_1" ;
      lv2:name "VMeter_1" ;
      lv2:default -70.000000 ;
      lv2:minimum -70.000000 ;
      lv2:maximum 6.000000 ;
   ], [
      a lv2:OutputPort ,
          lv2:ControlPort ;
      lv2:index 10 ;
      lv2:symbol "VMeter_2" ;
      lv2:name "VMeter_2" ;
      lv2:default -70.000000 ;
      lv2:minimum -70.000000 ;
      lv2:maximum 6.000000 ;
   ], [
      a lv2:OutputPort ,
          lv2:ControlPort ;
      lv2:index 11 ;
      lv2:symbol "VMeter_3" ;
      lv2:name "VMeter_3" ;
      lv2:default -70.000000 ;
      lv2:minimum -70.000000 ;
      lv2:maximum 6.000000 ;
   ], [
      a lv2:OutputPort ,
          lv2:ControlPort ;
      lv2:index 12 ;
      lv2:symbol "VMeter_4" ;
      lv2:name "VMeter_4" ;
      lv2:default -70.000000 ;
      lv2:minimum -70.000000 ;
      lv2:maximum 6.000000 ;
   ] .

<urn:mixer_ui>
   a guiext:X11UI;
   guiext:binary <Mixer_ui.so> ;
       lv2:extensionData guiext::idle ;
       lv2:extensionData guiext:resize ;
       lv2:extensionData guiext:idleInterface ;
       lv2:requiredFeature guiext:idleInterface ;
   .
